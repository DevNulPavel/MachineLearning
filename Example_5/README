export C_INCLUDE_PATH=/usr/local/include:$C_INCLUDE_PATH
export LIBRARY_PATH=/usr/local/lib:$LIBRARY_PATH

set -g -x C_INCLUDE_PATH /usr/local/include $C_INCLUDE_PATH
set -g -x LIBRARY_PATH /usr/local/lib $LIBRARY_PATH

pipenv install